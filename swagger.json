{
    "openapi": "3.0.0",
    "info": {
      "title": "API de Usuários e Peixes",
      "description": "Documentação da API para gerenciamento de usuários e peixes",
      "version": "1.0.0"
    },
    "servers": [
      {
        "url": "https://api-peixes.vercel.app",
        "description": "Servidor da API"
      }
    ],
    "paths": {
      "/peixes": {
        "post": {
          "summary": "Adicionar um novo peixe",
          "description": "Adiciona um novo peixe à coleção de peixes do usuário",
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Peixe"
                }
              }
            }
          },
          "responses": {
            "201": {
              "description": "Peixe adicionado com sucesso"
            },
            "400": {
              "description": "Requisição inválida"
            },
            "500": {
              "description": "Erro interno do servidor"
            }
          }
        }
      }
    },
    "components": {
      "schemas": {
        "Peixe": {
          "type": "object",
          "required": [
            "ID_usuario",
            "Especie",
            "Alimentacao",
            "Quantidade",
            "Tempo_alimentacao",
            "Imagem"
          ],
          "properties": {
            "ID_usuario": {
              "type": "string"
            },
            "Especie": {
              "type": "string"
            },
            "Alimentacao": {
              "type": "string"
            },
            "Quantidade": {
              "type": "number"
            },
            "Tempo_alimentacao": {
              "type": "string"
            },
            "Imagem": {
              "type": "string"
            }
          }
        }
      }
    }
  }
