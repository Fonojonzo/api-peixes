{
    "swagger": "2.0",
    "info": {
      "title": "API de Gerenciamento de Peixes",
      "version": "1.0.0",
      "description": "API para gerenciar informações de peixes."
    },
    "basePath": "/api",
    "paths": {
      "/peixes": {
        "post": {
          "summary": "Adicionar um novo peixe",
          "parameters": [
            {
              "name": "body",
              "in": "body",
              "schema": {
                "type": "object",
                "properties": {
                  "Especie": {
                    "type": "string",
                    "example": "Tilápia"
                  },
                  "Nome": {
                    "type": "string",
                    "example": "Nemo"
                  },
                  "Tempo_alimentacao": {
                    "type": "string",
                    "example": "2 vezes ao dia"
                  },
                  "Quantidade": {
                    "type": "integer",
                    "example": 5
                  },
                  "Alimentacao": {
                    "type": "string",
                    "example": "Ração"
                  },
                  "Imagem": {
                    "type": "string",
                    "example": "https://url_da_imagem.com/imagem.jpg"
                  },
                  "ID_usuario": {
                    "type": "string",
                    "example": "123456"
                  }
                }
              }
            }
          ],
          "responses": {
            "201": {
              "description": "Peixe adicionado com sucesso"
            },
            "400": {
              "description": "Requisição inválida"
            },
            "500": {
              "description": "Erro ao adicionar peixe"
            }
          }
        }
      },
      "/peixes/{id}": {
        "get": {
          "summary": "Buscar um peixe por ID",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "type": "string",
              "description": "ID do peixe a ser buscado"
            }
          ],
          "responses": {
            "200": {
              "description": "Peixe encontrado",
              "schema": {
                "type": "object",
                "properties": {
                  "Especie": {
                    "type": "string",
                    "example": "Tilápia"
                  },
                  "Nome": {
                    "type": "string",
                    "example": "Nemo"
                  },
                  "Tempo_alimentacao": {
                    "type": "string",
                    "example": "2 vezes ao dia"
                  },
                  "Quantidade": {
                    "type": "integer",
                    "example": 5
                  },
                  "Alimentacao": {
                    "type": "string",
                    "example": "Ração"
                  },
                  "Imagem": {
                    "type": "string",
                    "example": "https://url_da_imagem.com/imagem.jpg"
                  },
                  "ID_usuario": {
                    "type": "string",
                    "example": "123456"
                  }
                }
              }
            },
            "404": {
              "description": "Peixe não encontrado"
            },
            "500": {
              "description": "Erro ao buscar peixe"
            }
          }
        },
        "put": {
          "summary": "Atualizar um peixe por ID",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "type": "string",
              "description": "ID do peixe a ser atualizado"
            },
            {
              "name": "body",
              "in": "body",
              "schema": {
                "type": "object",
                "properties": {
                  "Especie": {
                    "type": "string",
                    "example": "Tilápia"
                  },
                  "Nome": {
                    "type": "string",
                    "example": "Nemo"
                  },
                  "Tempo_alimentacao": {
                    "type": "string",
                    "example": "2 vezes ao dia"
                  },
                  "Quantidade": {
                    "type": "integer",
                    "example": 5
                  },
                  "Alimentacao": {
                    "type": "string",
                    "example": "Ração"
                  },
                  "Imagem": {
                    "type": "string",
                    "example": "https://url_da_imagem.com/imagem.jpg"
                  }
                }
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Peixe atualizado com sucesso"
            },
            "404": {
              "description": "Peixe não encontrado"
            },
            "500": {
              "description": "Erro ao atualizar peixe"
            }
          }
        },
        "delete": {
          "summary": "Deletar um peixe por ID",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "type": "string",
              "description": "ID do peixe a ser deletado"
            }
          ],
          "responses": {
            "204": {
              "description": "Peixe deletado com sucesso"
            },
            "404": {
              "description": "Peixe não encontrado"
            },
            "500": {
              "description": "Erro ao deletar peixe"
            }
          }
        }
      }
    }
  }
  